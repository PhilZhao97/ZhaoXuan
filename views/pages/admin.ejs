
<div class="homepage-wrapper">
  <div class="left-banner">
    <h1 class="title">
      Q-Pon
    </h1>
    <h2 class="subtitle">
      Redeem your coins for dining coupons.
    </h2>
  </div>
  <div class="right-banner">
    Admin Page
  </div>
</div>

<div style="padding: 0 5rem">
  <table class="table is-bordered is-striped is-narrow is-hoverable is-fullwidth">
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Restaurant</th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% for (let dining of allDining) { %>
        <tr>
          <th><%= dining.id %></th>
          <td><%= dining.title %></td>
          <td><%= dining.restaurant %></td>
          <td><a href="/dining/update/<%= dining.id %>">Update</a></td>
          <td><a href="/dining/redeemed/<%= dining.id %>">Redeemed Members</a></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</div>

